{"ast":null,"code":"import availableParts from \"../data/part\";\n\nfunction getPreviousValidIndex(index = 0, length = 0) {\n  const deprecatedIndex = index - 1;\n  return deprecatedIndex < 0 ? length - 1 : deprecatedIndex;\n}\n\nfunction getNextValidIndex(index = 0, length = 0) {\n  const incrementedIndex = index + 1;\n  return incrementedIndex > length - 1 ? 0 : incrementedIndex;\n}\n\nexport default {\n  name: \"RobotBuilderPage\",\n\n  data() {\n    return {\n      cart: [],\n      availableParts,\n      headIndex: 0,\n      armIndex: 0,\n      torsosIndex: 0,\n      baseIndex: 0\n    };\n  },\n\n  computed: {\n    selectedRobot() {\n      return {\n        head: this.availableParts.heads[this.headIndex],\n        arm: this.availableParts.arms[this.armIndex],\n        torso: this.availableParts.torsos[this.torsosIndex],\n        base: this.availableParts.bases[this.baseIndex]\n      };\n    }\n\n  },\n  methods: {\n    addToCart() {\n      const cost = this.selectedRobot.head.cost + this.selectedRobot.arm.cost + this.selectedRobot.torso.cost + this.selectedRobot.base;\n    },\n\n    selectNextHead() {\n      this.headIndex = getNextValidIndex(this.headIndex, this.availableParts.heads.length);\n    },\n\n    selectPreviousHead() {\n      this.headIndex = getPreviousValidIndex(this.headIndex, this.availableParts.heads.length);\n    },\n\n    selectedNextArm() {\n      this.armIndex = getNextValidIndex(this.armIndex, this.availableParts.arms.length);\n    },\n\n    selectPreviousArm() {\n      this.armIndex = getPreviousValidIndex(this.armIndex, this.availableParts.arms.length);\n    },\n\n    selectNextTorsos() {\n      this.torsosIndex = getNextValidIndex(this.torsosIndex, this.availableParts.torsos.length);\n    },\n\n    selectPreviousTorsos() {\n      this.torsosIndex = getPreviousValidIndex(this.torsosIndex, this.availableParts.torsos.length);\n    },\n\n    selectNextBase() {\n      this.baseIndex = getNextValidIndex(this.baseIndex, this.availableParts.bases.length);\n    },\n\n    selectPreviousBase() {\n      this.baseIndex = getPreviousValidIndex(this.baseIndex, this.availableParts.bases.length);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0DA,OAAOA,cAAP,MAA2B,cAA3B;;AAEA,SAASC,qBAAT,CAA+BC,KAAI,GAAI,CAAvC,EAA0CC,MAAK,GAAI,CAAnD,EAAsD;EACpD,MAAMC,eAAc,GAAIF,KAAI,GAAI,CAAhC;EACA,OAAOE,eAAc,GAAI,CAAlB,GAAsBD,MAAK,GAAI,CAA/B,GAAmCC,eAA1C;AACF;;AAEA,SAASC,iBAAT,CAA2BH,KAAI,GAAI,CAAnC,EAAsCC,MAAK,GAAI,CAA/C,EAAkD;EAChD,MAAMG,gBAAe,GAAIJ,KAAI,GAAI,CAAjC;EACA,OAAOI,gBAAe,GAAIH,MAAK,GAAI,CAA5B,GAAgC,CAAhC,GAAoCG,gBAA3C;AACF;;AAEA,eAAe;EACbC,IAAI,EAAE,kBADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EADD;MAELT,cAFK;MAGLU,SAAS,EAAE,CAHN;MAILC,QAAQ,EAAE,CAJL;MAKLC,WAAW,EAAE,CALR;MAMLC,SAAS,EAAE;IANN,CAAP;EAQD,CAXY;;EAcbC,QAAQ,EAAE;IACRC,aAAa,GAAE;MACb,OAAO;QACLC,IAAI,EAAE,KAAKhB,cAAL,CAAoBiB,KAApB,CAA0B,KAAKP,SAA/B,CADD;QAELQ,GAAG,EAAE,KAAKlB,cAAL,CAAoBmB,IAApB,CAAyB,KAAKR,QAA9B,CAFA;QAGLS,KAAK,EAAE,KAAKpB,cAAL,CAAoBqB,MAApB,CAA2B,KAAKT,WAAhC,CAHF;QAILU,IAAI,EAAE,KAAKtB,cAAL,CAAoBuB,KAApB,CAA0B,KAAKV,SAA/B;MAJD,CAAP;IAMD;;EARO,CAdG;EAyBbW,OAAO,EAAE;IACPC,SAAS,GAAG;MACV,MAAMC,IAAG,GAAG,KAAKX,aAAL,CAAmBC,IAAnB,CAAwBU,IAAxB,GAA+B,KAAKX,aAAL,CAAmBG,GAAnB,CAAuBQ,IAAtD,GAA6D,KAAKX,aAAL,CAAmBK,KAAnB,CAAyBM,IAAtF,GAA6F,KAAKX,aAAL,CAAmBO,IAA5H;IACD,CAHM;;IAKPK,cAAc,GAAG;MACf,KAAKjB,SAAL,GAAiBL,iBAAiB,CAChC,KAAKK,SAD2B,EAEhC,KAAKV,cAAL,CAAoBiB,KAApB,CAA0Bd,MAFM,CAAlC;IAID,CAVM;;IAYPyB,kBAAkB,GAAG;MACnB,KAAKlB,SAAL,GAAiBT,qBAAqB,CACpC,KAAKS,SAD+B,EAEpC,KAAKV,cAAL,CAAoBiB,KAApB,CAA0Bd,MAFU,CAAtC;IAID,CAjBM;;IAmBP0B,eAAe,GAAG;MAChB,KAAKlB,QAAL,GAAgBN,iBAAiB,CAC/B,KAAKM,QAD0B,EAE/B,KAAKX,cAAL,CAAoBmB,IAApB,CAAyBhB,MAFM,CAAjC;IAID,CAxBM;;IAyBP2B,iBAAiB,GAAG;MAClB,KAAKnB,QAAL,GAAgBV,qBAAqB,CACnC,KAAKU,QAD8B,EAEnC,KAAKX,cAAL,CAAoBmB,IAApB,CAAyBhB,MAFU,CAArC;IAID,CA9BM;;IA+BP4B,gBAAgB,GAAG;MACjB,KAAKnB,WAAL,GAAmBP,iBAAiB,CAClC,KAAKO,WAD6B,EAElC,KAAKZ,cAAL,CAAoBqB,MAApB,CAA2BlB,MAFO,CAApC;IAID,CApCM;;IAqCP6B,oBAAoB,GAAG;MACrB,KAAKpB,WAAL,GAAmBX,qBAAqB,CACtC,KAAKW,WADiC,EAEtC,KAAKZ,cAAL,CAAoBqB,MAApB,CAA2BlB,MAFW,CAAxC;IAID,CA1CM;;IA2CP8B,cAAc,GAAG;MACf,KAAKpB,SAAL,GAAiBR,iBAAiB,CAChC,KAAKQ,SAD2B,EAEhC,KAAKb,cAAL,CAAoBuB,KAApB,CAA0BpB,MAFM,CAAlC;IAID,CAhDM;;IAiDP+B,kBAAkB,GAAG;MACnB,KAAKrB,SAAL,GAAiBZ,qBAAqB,CACpC,KAAKY,SAD+B,EAEpC,KAAKb,cAAL,CAAoBuB,KAApB,CAA0BpB,MAFU,CAAtC;IAID;;EAtDM;AAzBI,CAAf","names":["availableParts","getPreviousValidIndex","index","length","deprecatedIndex","getNextValidIndex","incrementedIndex","name","data","cart","headIndex","armIndex","torsosIndex","baseIndex","computed","selectedRobot","head","heads","arm","arms","torso","torsos","base","bases","methods","addToCart","cost","selectNextHead","selectPreviousHead","selectedNextArm","selectPreviousArm","selectNextTorsos","selectPreviousTorsos","selectNextBase","selectPreviousBase"],"sourceRoot":"","sources":["/Users/ahmedbilal/Documents/Prosonal Projects/bot-market/src/pages/RobotBuilderPage.vue"],"sourcesContent":["<template>\n  <div class=\"content\">\n    <button class=\"add-to-cart\" @click=\"addToCart()\">\n      Add To Card\n    </button>\n    <div class=\"top-row\">\n      <div class=\"top part\">\n        <div class=\"robot-name\">{{selectedRobot.head.title}}\n        <span v-if=\"selectedRobot.head.onSale\" class=\"sale\">Sale...!</span>\n        </div>\n        <img :src=\"selectedRobot.head.src\" title=\"head\" />\n        <button @click=\"selectPreviousHead()\" class=\"prev-selector\">\n          &#9668;\n        </button>\n        <button @click=\"selectNextHead()\" class=\"next-selector\">\n          &#9658;\n        </button>\n      </div>\n    </div>\n    <div class=\"middle-row\">\n      <div class=\"left part\">\n        <img :src=\"selectedRobot.arm.src\" title=\"left arm\" />\n        <button @click=\"selectPreviousArm()\" class=\"prev-selector\">\n          &#9650;\n        </button>\n        <button @click=\"selectedNextArm()\" class=\"next-selector\">\n          &#9660;\n        </button>\n      </div>\n      <div class=\"center part\">\n        <img\n          :src=\"selectedRobot.torso.src\"\n          title=\"left arm\"\n        />\n        <button @click=\"selectPreviousTorsos()\" class=\"prev-selector\">\n          &#9668;\n        </button>\n        <button @click=\"selectNextTorsos()\" class=\"next-selector\">\n          &#9658;\n        </button>\n      </div>\n      <div class=\"right part\">\n        <img :src=\"selectedRobot.arm.src\" title=\"left arm\" />\n        <button @click=\"selectPreviousArm()\" class=\"prev-selector\">&#9650;</button>\n        <button @click=\"selectedNextArm()\" class=\"next-selector\">&#9660;</button>\n      </div>\n    </div>\n    <div class=\"bottom-row\">\n      <div class=\"bottom part\">\n        <img :src=\"selectedRobot.base.src\" title=\"left arm\" />\n        <button @click=\"selectPreviousBase()\" class=\"prev-selector\">&#9668;</button>\n        <button @click=\"selectNextBase()\" class=\"next-selector\">&#9658;</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport availableParts from \"../data/part\";\n\nfunction getPreviousValidIndex(index = 0, length = 0) {\n  const deprecatedIndex = index - 1;\n  return deprecatedIndex < 0 ? length - 1 : deprecatedIndex;\n}\n\nfunction getNextValidIndex(index = 0, length = 0) {\n  const incrementedIndex = index + 1;\n  return incrementedIndex > length - 1 ? 0 : incrementedIndex;\n}\n\nexport default {\n  name: \"RobotBuilderPage\",\n  data() {\n    return {\n      cart: [],\n      availableParts,\n      headIndex: 0,\n      armIndex: 0,\n      torsosIndex: 0,\n      baseIndex: 0,\n    };\n  },\n\n\n  computed: {\n    selectedRobot(){\n      return {\n        head: this.availableParts.heads[this.headIndex],\n        arm: this.availableParts.arms[this.armIndex],\n        torso: this.availableParts.torsos[this.torsosIndex],\n        base: this.availableParts.bases[this.baseIndex]\n      }\n    },\n  },\n\n  methods: {\n    addToCart() {\n      const cost =this.selectedRobot.head.cost + this.selectedRobot.arm.cost + this.selectedRobot.torso.cost + this.selectedRobot.base;\n    },\n\n    selectNextHead() {\n      this.headIndex = getNextValidIndex(\n        this.headIndex,\n        this.availableParts.heads.length\n      );\n    },\n\n    selectPreviousHead() {\n      this.headIndex = getPreviousValidIndex(\n        this.headIndex,\n        this.availableParts.heads.length\n      );\n    },\n\n    selectedNextArm() {\n      this.armIndex = getNextValidIndex(\n        this.armIndex,\n        this.availableParts.arms.length\n      );\n    },\n    selectPreviousArm() {\n      this.armIndex = getPreviousValidIndex(\n        this.armIndex,\n        this.availableParts.arms.length\n      );\n    },\n    selectNextTorsos() {\n      this.torsosIndex = getNextValidIndex(\n        this.torsosIndex,\n        this.availableParts.torsos.length\n      );\n    },\n    selectPreviousTorsos() {\n      this.torsosIndex = getPreviousValidIndex(\n        this.torsosIndex,\n        this.availableParts.torsos.length\n      );\n    },\n    selectNextBase() {\n      this.baseIndex = getNextValidIndex(\n        this.baseIndex,\n        this.availableParts.bases.length\n      );\n    },\n    selectPreviousBase() {\n      this.baseIndex = getPreviousValidIndex(\n        this.baseIndex,\n        this.availableParts.bases.length\n      );\n    },\n  },\n};\n</script>\n\n<style scoped>\n.part {\n  position: relative;\n  width: 165px;\n  height: 165px;\n  border: 3px solid #aaa;\n}\n\n.part img {\n  width: 165px;\n}\n\n.top-row {\n  display: flex;\n  justify-content: space-around;\n}\n\n.middle-row {\n  display: flex;\n  justify-content: center;\n}\n\n.bottom-row {\n  display: flex;\n  justify-content: space-around;\n  border-top: none;\n}\n\n.head {\n  border-bottom: none;\n}\n\n.left {\n  border-right: none;\n}\n\n.right {\n  border-left: none;\n}\n\n.left img {\n  transform: rotate(-90deg);\n}\n\n.right img {\n  transform: rotate(90deg);\n}\n\n.bottom {\n  border-top: none;\n}\n\n.prev-selector {\n  position: absolute;\n  z-index: 1;\n  top: -3px;\n  left: -28px;\n  width: 25px;\n  height: 171px;\n}\n\n.next-selector {\n  position: absolute;\n  z-index: 1;\n  top: -3px;\n  right: -28px;\n  width: 25px;\n  height: 171px;\n}\n\n.center .prev-selector,\n.center .next-selector {\n  opacity: 0.8;\n}\n\n.left .prev-selector {\n  top: -28px;\n  left: -3px;\n  width: 144px;\n  height: 25px;\n}\n\n.left .next-selector {\n  top: auto;\n  bottom: -28px;\n  left: -3px;\n  width: 144px;\n  height: 25px;\n}\n\n.right .prev-selector {\n  top: -28px;\n  left: 24px;\n  width: 144px;\n  height: 25px;\n}\n\n.right .next-selector {\n  top: auto;\n  bottom: -28px;\n  left: 24px;\n  width: 144px;\n  height: 25px;\n}\n\n.right .next-selector {\n  right: -3px;\n}\n\n.robot-name {\n  position: absolute;\n  top: -25px;\n  text-align: center;\n  width: 100%;\n}\n.sale {\n  color: red;\n}\n\n.content {\n  position: relative;\n}\n\n.add-to-cart {\n  position: absolute;\n  right: 30px;\n  width: 220px;\n  padding: 3px;\n  font-size: 16px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}