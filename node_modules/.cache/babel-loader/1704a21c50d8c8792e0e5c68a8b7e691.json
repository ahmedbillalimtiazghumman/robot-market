bleParts.torsos.length);\n    },\n\n    selectNextBase() {\n      this.baseIndex = getNextValidIndex(this.baseIndex, this.availableParts.bases.length);\n    },\n\n    selectPreviousBase() {\n      this.baseIndex = getPreviousValidIndex(this.baseIndex, this.availableParts.bases.length);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4DA,OAAOA,cAAP,MAA2B,cAA3B;;AAEA,SAASC,qBAAT,CAA+BC,KAAI,GAAI,CAAvC,EAA0CC,MAAK,GAAI,CAAnD,EAAsD;EACpD,MAAMC,eAAc,GAAIF,KAAI,GAAI,CAAhC;EACA,OAAOE,eAAc,GAAI,CAAlB,GAAsBD,MAAK,GAAI,CAA/B,GAAmCC,eAA1C;AACF;;AAEA,SAASC,iBAAT,CAA2BH,KAAI,GAAI,CAAnC,EAAsCC,MAAK,GAAI,CAA/C,EAAkD;EAChD,MAAMG,gBAAe,GAAIJ,KAAI,GAAI,CAAjC;EACA,OAAOI,gBAAe,GAAIH,MAAK,GAAI,CAA5B,GAAgC,CAAhC,GAAoCG,gBAA3C;AACF;;AAEA,eAAe;EACbC,IAAI,EAAE,kBADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EADD;MAELT,cAFK;MAGLU,SAAS,EAAE,CAHN;MAILC,QAAQ,EAAE,CAJL;MAKLC,WAAW,EAAE,CALR;MAMLC,SAAS,EAAE;IANN,CAAP;EAQD,CAXY;;EAabC,QAAQ,EAAE;IACRC,aAAa,GAAG;MACd,OAAO;QACLC,IAAI,EAAE,KAAKhB,cAAL,CAAoBiB,KAApB,CAA0B,KAAKP,SAA/B,CADD;QAELQ,GAAG,EAAE,KAAKlB,cAAL,CAAoBmB,IAApB,CAAyB,KAAKR,QAA9B,CAFA;QAGLS,KAAK,EAAE,KAAKpB,cAAL,CAAoBqB,MAApB,CAA2B,KAAKT,WAAhC,CAHF;QAILU,IAAI,EAAE,KAAKtB,cAAL,CAAoBuB,KAApB,CAA0B,KAAKV,SAA/B;MAJD,CAAP;IAMF;;EARQ,CAbG;EAwBbW,OAAO,EAAE;IACPC,SAAS,GAAG;MACV,MAAMC,IAAG,GACL,KAAKX,aAAL,CAAmBC,IAAnB,CAAwBU,IAAxB,GACA,KAAKX,aAAL,CAAmBG,GAAnB,CAAuBQ,IADvB,GAEA,KAAKX,aAAL,CAAmBK,KAAnB,CAAyBM,IAFzB,GAGA,KAAKX,aAAL,CAAmBO,IAJvB,CADU,CAMV;;MACA,KAAKb,IAAL,CAAUkB,IAAV,CAAe,EAAC,GAAG,KAAKZ,aAAT;QAAwBW;MAAxB,CAAf;IACD,CATM;;IAWPE,cAAc,GAAG;MACf,KAAKlB,SAAL,GAAiBL,iBAAiB,CAC9B,KAAKK,SADyB,EAE9B,KAAKV,cAAL,CAAoBiB,KAApB,CAA0Bd,MAFI,CAAlC;IAID,CAhBM;;IAkBP0B,kBAAkB,GAAG;MACnB,KAAKnB,SAAL,GAAiBT,qBAAqB,CAClC,KAAKS,SAD6B,EAElC,KAAKV,cAAL,CAAoBiB,KAApB,CAA0Bd,MAFQ,CAAtC;IAID,CAvBM;;IAyBP2B,eAAe,GAAG;MAChB,KAAKnB,QAAL,GAAgBN,iBAAiB,CAC7B,KAAKM,QADwB,EAE7B,KAAKX,cAAL,CAAoBmB,IAApB,CAAyBhB,MAFI,CAAjC;IAID,CA9BM;;IA+BP4B,iBAAiB,GAAG;MAClB,KAAKpB,QAAL,GAAgBV,qBAAqB,CACjC,KAAKU,QAD4B,EAEjC,K